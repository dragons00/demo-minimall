swiper {
  width: 750rpx;
  height: 340rpx;
}
swiper image {
  width: 100%;
}
.category {
  display: flex;
  justify-content: space-between;
}
.category navigator {
  flex: 1;
  padding: 20rpx;
}
.category image {
  width: 100%;
}
.floor-title {
  padding: 10rpx 0;
}
.floor-content {
  clear: both;
}
.floor-content navigator {
  float: left;
  width: 33.33%;
}
.floor-content navigator:nth-last-child(-n + 4) {
  height: 207.953340526rpx;
  border-left: 10rpx solid #fff;
}
.floor-content navigator:nth-child(2),
.floor-content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor-content navigator image {
  width: 100%;
  height: 100%;
}
